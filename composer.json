{
    "name": "co-stack/fal-gallery",
    "description": "Easy to use folder based gallery with separate single-, category- and list view that supports any TYPO3 FAL driver. Official successor of wt_gallery",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0",
    "keywords": [
        "TYPO3",
        "FAL",
        "Gallery",
        "wt_gallery"
    ],
    "authors": [
        {
            "name": "Oliver Eglseder",
            "email": "php@vxvr.de"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-pdo": "*",
        "typo3/cms-core": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "CoStack\\FalGallery\\": "Classes/"
        }
    },
    "suggest": {
        "ext-exif": "Install the PHP EXIF extension to display exif data of your pictures"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "fal_gallery"
        }
    }
}
